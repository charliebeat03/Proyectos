.rt-Kbd:where(button):where(:active:not([data-state="open"])) { padding-top: 0.05em; padding-bottom: 0px; transition-duration: 40ms, 40ms; background-color: var(--gray-2); box-shadow: inset 0 .05em var(--black-a3),0 0 0 .05em var(--gray-a7); }

.rt-Kbd:where(:any-link, button):where(:active:not([data-state="open"])) { padding-top: 0.05em; padding-bottom: 0px; transition-duration: 40ms, 40ms; background-color: var(--gray-2); box-shadow: inset 0 .05em var(--black-a3),0 0 0 .05em var(--gray-a7); }

