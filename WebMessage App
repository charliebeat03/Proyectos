@media (prefers-reduced-motion: no-preference) {
  @keyframes rt-dialog-overlay-no-op { 
  0% { opacity: 1; }
  100% { opacity: 1; }
}
  @keyframes rt-dialog-content-show { 
  0% { opacity: 0; transform: translateY(5px) scale(0.97); }
  100% { opacity: 1; transform: translateY(0px) scale(1); }
}
  @keyframes rt-dialog-content-hide { 
  0% { opacity: 1; transform: translateY(0px) scale(1); }
  100% { opacity: 0; transform: translateY(5px) scale(0.99); }
}
  .rt-BaseDialogOverlay:where([data-state="closed"]) { animation: 0.16s cubic-bezier(0.16, 1, 0.3, 1) 0s 1 normal none running rt-dialog-overlay-no-op; }
  .rt-BaseDialogOverlay:where([data-state="open"])::before { animation: 0.2s cubic-bezier(0.16, 1, 0.3, 1) 0s 1 normal none running rt-fade-in; }
  .rt-BaseDialogOverlay:where([data-state="closed"])::before { animation: 0.16s cubic-bezier(0.16, 1, 0.3, 1) 0s 1 normal none running rt-fade-out; }
  .rt-BaseDialogContent:where([data-state="open"]) { animation: 0.2s cubic-bezier(0.16, 1, 0.3, 1) 0s 1 normal none running rt-dialog-content-show; }
  .rt-BaseDialogContent:where([data-state="closed"]) { animation: 0.1s cubic-bezier(0.16, 1, 0.3, 1) 0s 1 normal none running rt-dialog-content-hide; }
}

