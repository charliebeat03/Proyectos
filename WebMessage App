@supports (color: color(display-p3 1 1 1)) {
  @media (color-gamut: p3) {
  .radix-themes { --color-transparent: color(display-p3 0 0 0 / 0); }
}
}

.radix-themes:where(.light, .light-theme), :root:where(:has(.radix-themes[data-is-root-theme="true"]:where(.light, .light-theme))) { color-scheme: light; }

.radix-themes:where(.dark, .dark-theme), :root:where(:has(.radix-themes[data-is-root-theme="true"]:where(.dark, .dark-theme))) { color-scheme: dark; }

